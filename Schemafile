create_table "lyrics", force: :cascade do |t|
  t.integer  "song_id"
  t.text     "new_lyric"
  t.datetime "created_at",   null: false
  t.datetime "updated_at",   null: false
end

create_table "songs", force: :cascade do |t|
  t.string   "song_name"
  t.string   "song_path"
  t.text     "lyric"
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
end

create_table "twitter_info", force: :cascade do |t|
  t.text   "context"
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
end
